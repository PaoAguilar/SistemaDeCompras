<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="Principal">
    <page>/Principal.jsf</page>
  </view>
  <view id="gestionarEmpleado">
    <page>/gestionarEmpleado.jsf</page>
  </view>
  <view id="gestionarDepartamento">
    <page>/gestionarDepartamento.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>gestionarEmpleado</from-outcome>
      <to-activity-id>gestionarEmpleado</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__4">
      <from-outcome>principal</from-outcome>
      <to-activity-id>Principal</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__5">
      <from-outcome>gestionarDepartamento</from-outcome>
      <to-activity-id>gestionarDepartamento</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
